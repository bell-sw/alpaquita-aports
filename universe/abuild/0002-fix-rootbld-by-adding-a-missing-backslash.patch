From 706067d09ab2af9b812ae6ef1c5e490629a75653 Mon Sep 17 00:00:00 2001
From: Kaarle Ritvanen <kunkku@alpinelinux.org>
Date: Sun, 13 Oct 2024 18:12:43 +0300
Subject: [PATCH] fix rootbld by adding a missing backslash

This fixes a regression in commit a7e7587758bdf36436966dfa0ba9b991e5651b67.
---
 abuild.in | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/abuild.in b/abuild.in
index 5503a09..9177ab7 100644
--- a/abuild.in
+++ b/abuild.in
@@ -2539,7 +2539,7 @@ rootbld() {
 		"$BUILD_ROOT/$ABUILD_USERDIR" "$BUILD_ROOT/$aportsgit" \
 		"$BUILD_ROOT/$SRCDEST" "$BUILD_ROOT/$REPODEST" \
 		"$BUILD_ROOT/tmp/pkg" "$BUILD_ROOT/tmp/src" \
-		"$BUILD_ROOT/tmp/tmp" "$BUILD_ROOT/usr/bin"
+		"$BUILD_ROOT/tmp/tmp" "$BUILD_ROOT/usr/bin" \
 		"$REPODEST" "$BUILD_ROOT/$HOME/.ccache"
 
 	cp /etc/abuild.conf /etc/group /etc/passwd "$BUILD_ROOT/etc"
-- 
2.46.2

