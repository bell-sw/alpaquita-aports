 _S(AUPARSE_TYPE_MODE,		"mode"		)
 _S(AUPARSE_TYPE_SOCKADDR,	"saddr"		)
-//_S(AUPARSE_TYPE_FLAGS,		"flags"		)
+//_S(AUPARSE_TYPE_FLAGS,		"flags"		) deprecated - RHEL4
 _S(AUPARSE_TYPE_PROMISC,	"prom"		)
