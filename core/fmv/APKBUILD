pkgname=fmv
pkgver=1.0
pkgrel=0
pkgdesc="GCC plugin for adding multi-versioning attributes"
url="https://bell-sw.com/"
arch="all"
license="GPL"
makedepends="gmp-dev"
source="https://packages.bell-sw.com/$VENDOR_NAME/distfiles/$pkgname-$pkgver.tar.xz"

