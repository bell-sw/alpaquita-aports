pkgname=fmv
pkgver=1.1
pkgrel=2
pkgdesc="GCC plugin for adding multi-versioning attributes"
url="$VENDOR_URL"
arch="x86_64"
license="GPL"
makedepends="gmp-dev"
source="${VENDOR_URL/\/\//\/\/packages.}${VENDOR_DISTRO}/distfiles/$pkgname-$pkgver.tar.xz"

build() {
	make
}

package() {
	make DESTDIR="$pkgdir" install
}

check() {
	make test
}

